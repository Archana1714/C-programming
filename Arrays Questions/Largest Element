#include <iostream>
#include<climits>
using namespace std;
int main(){

//take array input from user

    int n;
    cin>>n;
    int input[100];
    
    for(int i=0;i<n;i++){
        cin>>input[i];
    }
    
    //print array
    
    for(int i=0;i<n;i++){
        cout<<input[i]<<endl;
    }
    
    //largest number in an array

     int max=INT_MIN;
     for(int i=0;i<n;i++){
         if (input[i]>max){
             max=input[i];
         }
     }
     cout<<max<<endl;
     
}
