/* Check whether number is Prime or not.  */


#include <iostream>
using namespace std;
int main()
{
  int n;
  
  //taking input
  
  cin>>n;
  bool divided = false;
  
  //loop
  
  for(int d=2;d<n;d++){
  
  //condition
  
      if(n%d==0){
          divided = true;
      }
  }
  
  //condition
  
  if(divided){
      cout<<"NOT PRIME";
  }
  else{
      cout<<"PRIME";
  }
}
