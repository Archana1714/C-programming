#include<iostream>
using namespace std;


int main(){
int rows, count = 0, count1, k =0;
 
 int i=1;
    cin>>rows;
    while(i <= rows)
    {
        while(k <= i-1)
        {
            if (count <= rows-1)
            {
                cout << i+k;
                ++count;
            }
             
            ++k;
        }
        count1 = count = k = 0;
 
        cout << endl;
        ++i;
    }
  
}


