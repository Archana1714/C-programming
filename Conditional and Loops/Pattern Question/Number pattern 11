/*    Print the following pattern for the given N number of rows.
Pattern for N = 4
1
21
321
4321 */

#include<iostream>
using namespace std;


int main(){
int n;

//taking input

    cin>>n;
    int i=1;
    int j;
  
  //loop1
  
    while(i<=n){
        j=1;
          int k=i;
          
          //loop2
          
        while(j<=i){
            cout<<k;
            k--;
                j++;
        }
        cout<<endl;
        i++;
    }
}
