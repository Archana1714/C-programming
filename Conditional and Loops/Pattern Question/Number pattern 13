#include<iostream>
using namespace std;


int main(){

     int n;
     
     //taking input
     
    cin>>n;
    int i=1;
    
    //loop1
    
    while(i<=n){
        int j=1;
        
        //loop2
        
        while(j<=n-i+1){
            cout<<n-i+1;
            j=j+1;
        }
        cout<<endl;
        i=i+1;
    }
  
}


