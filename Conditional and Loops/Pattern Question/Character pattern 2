#include<iostream>
using namespace std;
int main(){
    int n;
    
    //taking input
    
    cin>>n;
    int i=1;
    int j;
    //loop1
    
    while(i<=n){
        j=1;
        char starch='A'+i-1;
        
         //loop2
         
        while(j<=n){
            char ch= starch +j-1;
            cout<<ch;
            j++;
        }
        cout<<endl;
        i++;
    }
}
