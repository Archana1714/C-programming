#include<iostream>
using namespace std;

int main() {
	char a;
  
  //taking input from user
  
    cin>>a;
    
    //condition for Uppercase
    
    if(a>= 65 && a<= 96){
        cout<<1<<endl;
    }
    
    //condition for Lowercase
    
    else if(a>= 97 && a<= 122){
        cout<<0<<endl;
    }
   
   //condition for character is not an alphabet
   
    else{
        cout<<-1<<endl;
    }
	
}
